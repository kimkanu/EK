{
  "name": "ek",
  "productName": "ek",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "preinstall": "./scripts/preinstall.sh",
    "postinstall": "./scripts/postinstall.sh"
  },
  "keywords": [],
  "author": "keonwoo",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ek"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/pages/login/template.html",
                  "js": "./src/pages/login/renderer.ts",
                  "name": "login"
                },
                {
                  "html": "./src/pages/register/template.html",
                  "js": "./src/pages/register/renderer.ts",
                  "name": "register"
                },
                {
                  "html": "./src/pages/channel_list/template.html",
                  "js": "./src/pages/channel_list/renderer.ts",
                  "name": "channel_list"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-deb": "^6.0.0-beta.52",
    "@electron-forge/maker-rpm": "^6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.52",
    "@electron-forge/maker-zip": "^6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/bson": "^4.0.2",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/node-forge": "^0.9.4",
    "@types/request-promise": "^4.1.46",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "css-loader": "^3.6.0",
    "electron": "9.1.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "jest": "^26.2.2",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^26.1.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@storycraft/node-kakao": "^3.0.3",
    "@types/virtual-dom": "^2.1.0",
    "axios": "^0.19.2",
    "bson": "^4.0.4",
    "dotenv": "^8.2.0",
    "dotenv-flow": "^3.2.0",
    "dotenv-json": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "json-proxy-mapper": "^1.1.2",
    "keytar": "^6.0.1",
    "lossless-json": "^1.0.4",
    "node-forge": "^0.9.1",
    "node-machine-id": "^1.1.12",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "virtual-dom": "^2.1.1"
  }
}
